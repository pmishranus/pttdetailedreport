{
    "_version": "1.12.0",
    "sap.app": {
        "id": "nus.edu.sg.pttdetailedreport",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "sourceTemplate": {
            "id": "ui5template.basicSAPUI5ApplicationProject",
            "version": "1.40.12",
            "toolsId": "8c05675f-41db-47de-8b15-ccfead854ba5"
        },
        "dataSources": {
            "mainService": {
                "uri": "/v2/catalog/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            },
            "utilService": {
                "uri": "/v2/util/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            },
            "eclaims": {
                "uri": "/v2/eclaims/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "nus.edu.sg.pttdetailedreport.view.report",
            "type": "XML",
            "async": true,
            "id": "report"
        },
        "dependencies": {
            "minUI5Version": "1.65.6",
            "libs": {
                "sap.ui.layout": {},
                "sap.ui.core": {},
                "sap.m": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "nus.edu.sg.pttdetailedreport.i18n.i18n"
                }
            },
            "": {
                "dataSource": "utilService",
                "preload": true,
                "settings": {
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "synchronizationMode": "None",
                    "earlyRequests": true
                }
            },
            "Catalog": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "synchronizationMode": "None",
                    "earlyRequests": true
                }
            },
            "Eclaims": {
              "dataSource": "eclaims",
              "preload": true,
              "settings": {
                "operationMode": "Server",
                "autoExpandSelect": true,
                "earlyRequests": true
              }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "nus.edu.sg.pttdetailedreport.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "Routereport",
                    "pattern": "Routereport",
                    "target": [
                        "Targetreport"
                    ]
                }
            ],
            "targets": {
                "Targetreport": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "report",
                    "viewName": "report"
                }
            }
        }
    },
    "sap.platform.hcp": {
        "uri": "webapp",
        "_version": "1.1.0"
    }
}