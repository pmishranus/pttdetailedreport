						<Text text="{EclaimSrvModel>HOURS}"/>
						<Text text="{EclaimSrvModel>RATE_TYPE_AMOUNT}"/>
						<Text text="{EclaimSrvModel>TOTAL_AMOUNT}"/>
						<Text text="{EclaimSrvModel>WBS}"/>
						<Text text="{EclaimSrvModel>REMARKS}"/>
						<!--<Text text="{EclaimSrvModel>SUBMITTED_ON}"/>-->
